// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Trail {
  id          String  @id @default(cuid())
  userId      String
  title       String
  description String
  distance    Float
  elevation   Float
  published   Boolean @default(false)
  geo         String
  gpx         String
}
